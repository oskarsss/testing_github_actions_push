{
  "name": "vektor-dashboard-frontend",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "preinstall": "bash ./scripts/setup-ssh.sh",
    "predev": "node -e \"try { require('fs').accessSync('.env', fs.constants.F_OK); } catch (e) { console.log('\\x1b[31m', 'Please add .env file. You may use .env.staging as a template', '\\x1b[0m'); process.exit(1); }\"",
    "dev": "nodemon --exec next dev",
    "build": "set NODE_ENV=production && next build",
    "prestart": "node -e \"try { require('fs').accessSync('.env', fs.constants.F_OK); } catch (e) { console.log('\\x1b[31m', 'Please add .env file. You may use .env.staging as a template', '\\x1b[0m'); process.exit(1); }\"",
    "start": "next start",
    "export": "next export",
    "lint": "eslint --cache --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src\"",
    "watch-tsc": "tsc --watch --noEmit",
    "gen-chat": "./chat/protogen.sh",
    "prepare": "husky install",
    "submodule-update": "git submodule init && git submodule update && sh ./prepare.sh",
    "update_proto": "bash ./update_proto.sh",
    "update_proto_data": "bash ./update_data_proto.sh",
    "test": "cross-env PORT=8080 start-server-and-test start 8080 \"cypress run\"",
    "test:push": "cross-env PORT=8080 start-server-and-test start 8080 \"cypress run --config video=false,screenshotOnRunFailure=false\"",
    "build:test": "env-cmd -f .env.test next build",
    "start:test": "env-cmd -f .env.test next start -p 8080",
    "eslint:check": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "ts:build": "tsc -p tsconfig.json --noEmit",
    "build-dictionary": "node ./style-dictionary/config.ts",
    "ui": "npm update vektor-ui "
  },
